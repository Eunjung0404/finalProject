<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mybatis.mapper.ChaTheaterMapper">
 <select id="areaCount" parameterType="string" resultType="int">
 select count(*) from theater where area=#{area}
 </select>
 <select id="selectTheater" parameterType="string" resultType="hashmap">
 select theatername from theater where area=#{area}
 </select>
</mapper>